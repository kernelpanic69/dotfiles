set $term alacritty
set $browser librewolf

# Start a terminal
bindsym $mod+Return exec $term

# Kill focused window
bindcode $mod+24 kill

# Start your launcher
bindcode $mod+40 exec wofi --show drun --allow-images --no-actions

bindcode $mod+28 exec telegram-desktop
bindcode $mod+25 exec $browser
bindcode $mod+29 exec keepassxc
bindcode $mod+30 exec skypeforlinux
bindcode $mod+26 exec nemo
