set $term alacritty
set $menu dmenu_run | xargs swaymsg exec --
set $browser librewolf

# Start a terminal
bindsym $mod+Return exec $term

# Kill focused window
bindsym $mod+q kill

# Start your launcher
bindsym $mod+d exec $menu

bindsym $mod+t exec telegram-desktop
bindsym $mod+w exec $browser
bindsym $mod+y exec keepassxc